{
  "name": "data_entry",
  "version": "1.0.0",
  "description": "Only data prompter",
  "main": "index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "test": "vitest",
    "build": "npx tsc",
    "dev:nodemon-build": "nodemon dist/index.js",
    "dev:nodemon": "nodemon --exec ts-node ./src/index.ts",
    "dev:loader": "node --loader ts-node/esm ./src/index.ts",
    "dev:build": "node ./build/src/index.js",
    "dev": "ts-node ./src/index.ts",
    "cron": "ts-node ./src/cron.ts",
    "dev:all": "concurrently \"npm run dev\"  \"npm run cron\"",
    "start": "node index.js",
    "start-cron": "ts-node ./src/cron.ts",
    "start:all": "concurrently \"npm run start\" \"npm run start-cron\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RadamantiumX/data_entry.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RadamantiumX/data_entry/issues"
  },
  "homepage": "https://github.com/RadamantiumX/data_entry#readme",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/node-cron": "^3.0.11",
    "@types/redis": "^4.0.10",
    "@types/redis-rate-limiter": "^1.2.6",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.1.2",
    "msw": "^2.7.5",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^3.1.2"
  }
